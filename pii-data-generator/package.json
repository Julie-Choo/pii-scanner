{
  "name": "pii-data-generator",
  "version": "1.0.0",
  "private": true,
  "description": "Generate realistic PII test datasets with medical codes",
  "keywords": ["pii", "data-generator", "privacy", "testing", "icd10"],
  "author": "Your Name",
  "license": "MIT",
  "main": "./build/electron.js",
  "homepage": "./",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "lucide-react": "^0.263.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "electron-builder",
    "preelectron-pack": "npm run build",
    "dist": "npm run build && electron-builder --publish=never"
  },
  "devDependencies": {
    "react-scripts": "5.0.1",
    "electron": "^latest",
    "electron-builder": "^latest",
    "electron-is-dev": "^latest",
    "concurrently": "^latest",
    "wait-on": "^latest"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.yourname.pii-data-generator",
    "productName": "PII Data Generator",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "electron.js"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": {
        "target": "dmg",
        "arch": ["x64", "arm64"]
      }
    },
    "win": {
      "target": {
        "target": "nsis",
        "arch": ["x64"]
      }
    },
    "linux": {
      "target": {
        "target": "AppImage",
        "arch": ["x64"]
      }
    }
  }
}